{
  "name": "afya_bora/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 7777",
    "build": "next build",
    "start": "next start -p 7777",
    "lint": "eslint --fix \"{,!(node_modules|.next)/**/}*.{js,jsx,ts,tsx}\"",
    "format": "prettier -w -u \"{,!(node_modules|.next)/**/}*.{js,jsx,ts,tsx}\"",
    "schemagenerate": "apollo client:download-schema --endpoint http://localhost:7771/api/graphql ./lib/graphql/generated/schema.graphqls",
    "codegen": "apollo client:codegen --target typescript --localSchemaFile ./lib/graphql/generated/schema.graphqls --outputFlat --includes \"lib/graphql/**/*.ts\" --excludes \"lib/graphql/generated/**\" --addTypename --globalTypesFile \"lib/graphql/generated/graphql-global-types.ts\" ./lib/graphql/generated",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.3.12",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@apollo/react-common": "^3.1.4",
    "@apollo/react-hooks": "^4.0.0",
    "@chakra-ui/icons": "^1.0.6",
    "@chakra-ui/react": "^1.3.4",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@stripe/react-stripe-js": "^1.4.0",
    "@stripe/stripe-js": "^1.13.2",
    "apollo-upload-client": "^14.1.3",
    "babel-core": "^6.26.3",
    "debounce": "^1.2.1",
    "downshift": "^6.1.1",
    "framer-motion": "^3.10.3",
    "graphql": "^15.5.0",
    "graphql-upload": "^11.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^10.0.8",
    "nprogress": "^0.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "^6.15.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/debounce": "^1.2.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.34",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "apollo": "^2.32.5",
    "babel-jest": "^26.6.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
}
